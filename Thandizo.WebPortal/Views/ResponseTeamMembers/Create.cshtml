@model Thandizo.DataModels.Core.ResponseTeamMemberDTO

<div class="row heading-bg  bg-th-grey">
    <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
        <h5 class="txt-dark">Create</h5>
    </div>
    <!-- Breadcrumb -->
    <div class="col-lg-9 col-sm-8 col-md-8 col-xs-12">
        <ol class="breadcrumb">
            <li><a asp-action="Dashboard" asp-controller="Home">Dashboard</a></li>
            <li><a asp-action="Index"><span>Response Team Members</span></a></li>
            <li><a class="active"><span>Create</span></a></li>
        </ol>
    </div>
    <!-- /Breadcrumb -->
</div>
<!-- /Title -->
<!-- Row -->
<div class="row">
    <div class="col-sm-12">
        <div class="panel panel-default card-view">
            <div class="panel-heading">

                <div class="clearfix"></div>
            </div>
            <div class="panel-wrapper collapse in">
                <form asp-action="Create" class="form-horizontal">
                    <input type="hidden" asp-for="CreatedBy" />
                    <div class="panel-body">
                        @{
                            Html.RenderPartial("../Shared/_MessageOutput");
                        }

                        @{
                            await Html.RenderPartialAsync("_DataEntry");
                        }

                    </div>
                    <div class="panel-footer panel-default">
                        <div class="text-right">
                            <div class="btn-group" role="group" aria-label="...">
                                <button class="btn btn-primary btn-sm" type="submit">
                                    <i class="fa fa-save"></i><span class="with-icon">Save</span>
                                </button>
                                <button class="btn btn-default btn-sm" type="button" onclick="location.href='@Url.Action("Index", "ResponseTeamMembers")'">
                                    <i class="fa fa-times"></i><span class="with-icon">Cancel</span>
                                </button>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
<!-- /Row -->
