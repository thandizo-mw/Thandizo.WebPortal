@model Thandizo.DataModels.Core.ResponseTeamMemberDTO


<div class="form-group row">
    <label class = "bold-text-600 col-md-3 control-label">First Name</label>

    <div class="col-md-6">
        <div class="input-group">
            <span class="input-group-addon"><i class="fa fa-pencil"></i></span>
            <input type="text" class="form-control input" placeholder="Enter name"
                   asp-for="FirstName" required>
        </div>
    </div>
</div>
<div class="form-group row">
    <label class = "bold-text-600 col-md-3 control-label">Other Names</label>

    <div class="col-md-6">
        <div class="input-group">
            <span class="input-group-addon"><i class="fa fa-pencil"></i></span>
            <input type="text" class="form-control input" placeholder="Enter name"
                   asp-for="OtherNames" required>
        </div>
    </div>
</div>
<div class="form-group row">
    <label class = "bold-text-600 col-md-3 control-label">Surname</label>

    <div class="col-md-6">
        <div class="input-group">
            <span class="input-group-addon"><i class="fa fa-pencil"></i></span>
            <input type="text" class="form-control input" placeholder="Enter name"
                   asp-for="Surname" required>
        </div>
    </div>
</div>
<div class="form-group row">
    <label class = "bold-text-600 col-md-3 control-label">Phone Number</label>

    <div class="col-md-6">
        <div class="input-group">
            <span class="input-group-addon"><i class="fa fa-phone"></i></span>
            <input type="text" class="form-control input" placeholder="Enter phone number"
                   asp-for="PhoneNumber" required>
        </div>
    </div>
</div>
<div class="form-group row">
    <label class = "bold-text-600 col-md-3 control-label">Email Address</label>

    <div class="col-md-6">
        <div class="input-group">
            <span class="input-group-addon"><i class="fa fa-at"></i></span>
            <input type="text" class="form-control input" placeholder="Enter email address"
                   asp-for="EmailAddress" required>
        </div>
    </div>
</div>