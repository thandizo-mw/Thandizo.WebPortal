@model Thandizo.DataModels.DataCenters.Responses.HealthCareWorkerResponse


<div class="row">
    <div class="col-md-3 text-right">
        <label class="bold-text-600 txt-dark">Name</label>
    </div><div class="col-md-9 txt-dark">
        @Html.DisplayFor(model => model.FirstName) @Html.DisplayFor(model => model.OtherNames) @Html.DisplayFor(model => model.LastName)
    </div>
</div>
<div class="row">
    <div class="col-md-3 text-right">
        <label class="bold-text-600 txt-dark">Data Center Name</label>
    </div><div class="col-md-9 txt-dark">
        @Html.DisplayFor(model => model.CenterName)
    </div>
</div>
<div class="row">
    <div class="col-md-3 text-right">
        <label class="bold-text-600 txt-dark">Date of Birth</label>
    </div><div class="col-md-9 txt-dark">
        @Model.DateOfBirth.ToString("dd-MMM-yyyy")
    </div>
</div>
<div class="row">
    <div class="col-md-3 text-right">
        <label class="bold-text-600 txt-dark">Gender</label>
    </div>
    @if (Model.Gender.Equals("F"))
    {
        <div class="col-md-9 txt-dark">
            Female
        </div>

    }
    else
    {
        <div class="col-md-9 txt-dark">
            Male
        </div>
    }
</div>
<div class="row">
    <div class="col-md-3 text-right">
        <label class="bold-text-600 txt-dark">Phone Number</label>
    </div><div class="col-md-9 txt-dark">
        @Html.DisplayFor(model => model.PhoneNumber)
    </div>
</div>
<div class="row">
    <div class="col-md-3 text-right">
        <label class="bold-text-600 txt-dark">Email Address</label>
    </div><div class="col-md-9 txt-dark">
        @Html.DisplayFor(model => model.EmailAddress)
    </div>
</div>
<div class="row">
    <div class="col-md-3 text-right">
        <label class="bold-text-600 txt-dark">Identification Type</label>
    </div><div class="col-md-9 txt-dark">
        @Html.DisplayFor(model => model.IdentitificationTypeName)
    </div>
</div>

<div class="row">
    <div class="col-md-3 text-right">
        <label class="bold-text-600 txt-dark">Indentification Number</label>
    </div><div class="col-md-9 txt-dark">
        @Html.DisplayFor(model => model.IdentificationNumber)
    </div>
</div>
