@model IEnumerable<Thandizo.DataModels.Patients.Responses.PatientDailyStatusResponse>

<div class="row heading-bg  bg-th-grey">
    <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
        <h5 class="txt-dark">Patient Reported Symptoms</h5>
    </div>
    <!-- Breadcrumb -->
    <div class="col-lg-9 col-sm-8 col-md-8 col-xs-12">
        <ol class="breadcrumb">
            <li><a asp-action="Dashboard" asp-controller="Home">Dashboard</a></li>
            <li><a asp-action="Index"><span>Reported Symptoms</span></a></li>
            <li><a class="active"><span>Patient Reported Symptoms</span></a></li>
        </ol>
    </div>
    <!-- /Breadcrumb -->
</div>
<!-- /Title -->
<div class="row">

    <div class="col-md-5 text-right col-lg-offset-7">
        <div class="btn-group" role="group" aria-label="...">
            <button class="btn btn-default btn-sm" type="button" onclick="location.href='@Url.Action("Index", "PatientDailyStatuses")'">
                <i class="fa fa-arrow-left"></i><span class="with-icon">Back</span>
            </button>
        </div>
    </div>
</div>
<!-- Row -->
<div class="row">
    <div class="col-sm-12">
        <div class="panel panel-default card-view">
            <div class="panel-wrapper collapse in">
                <div class="panel-body">
                    @{
                        Html.RenderPartial("../Shared/_MessageOutput");
                    }
                    <div class="table-wrap">
                        <div class="table-responsive">
                            <table id="dtDataTable" class="table table-hover display  pb-30 dt-dataTable">
                                <thead>
                                    <tr>
                                        <th>Symptom Name</th>
                                        <th class="table-th-250">Date Submitted</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @if (Model is null)
                                    {
                                        <tr>
                                            <td colspan="5">
                                                <div class="text-center">No Data Found</div>
                                            </td>
                                        </tr>
                                    }
                                    else
                                    {
                                        @foreach (var item in Model)
                                        {
                                            <tr>
                                                <td>@Html.DisplayFor(modelItem => item.SymptomName)</td>
                                                <td>@item.DateSubmitted.ToString("dd-MMM-yyyy hh:mm tt")</td>
                                            </tr>
                                        }
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- /Row -->
