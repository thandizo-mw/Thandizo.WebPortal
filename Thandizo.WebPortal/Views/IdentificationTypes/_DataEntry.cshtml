@model Thandizo.DataModels.Core.IdentificationTypeDTO


<div class="form-group row">
    <label class="bold-text-600 col-md-3 control-label">Description</label>

    <div class="col-md-6">
        <div class="input-group">
            <span class="input-group-addon"><i class="fa fa-pencil"></i></span>
            <input type="text" class="form-control input" placeholder="Enter name"
                   asp-for="Description" required>
        </div>
    </div>
</div>

<div class="form-group row">
    <label class="bold-text-600 col-md-3 control-label">External Reference Number</label>

    <div class="col-md-6">
        <div class="input-group">
            <span class="input-group-addon"><i class="fa fa-pencil"></i></span>
            <input type="text" class="form-control input" placeholder="Enter number"
                   asp-for="ExternalReferenceNumber" required>
        </div>
    </div>
</div>